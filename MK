    UiSettingsModule:SetTeleportFruit(state)
    Settings["TeleportFruit"] = state
end)

local AutoKenToggle = Section:NewToggle("ãƒ»Auto Ken", " AutoKen", function(state)
    SilentAimModule:SetAutoKen(state)
    Settings["AutoKen"] = state
